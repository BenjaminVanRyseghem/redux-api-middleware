{
  "name": "redux-api-middleware",
  "version": "3.0.0-beta.0",
  "description": "Redux middleware for calling an API.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "umd/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "babel src --out-dir es && rollup -c",
    "postbuild": "npm run size",
    "clean": "rimraf es lib umd coverage",
    "test": "BABEL_ENV=test babel-node test/index.js | tap-spec",
    "cover": "BABEL_ENV=test babel-node ./node_modules/.bin/babel-istanbul cover test/index.js | tap-spec",
    "lint": "eslint src test",
    "prepublishOnly": "npm run lint && npm test && npm run clean && npm run build",
    "size": "size-limit"
  },
  "repository": "agraboso/redux-api-middleware",
  "homepage": "https://github.com/agraboso/redux-api-middleware",
  "keywords": [
    "redux",
    "api",
    "middleware",
    "redux-middleware",
    "flux"
  ],
  "author": {
    "name": "Alberto Garcia-Raboso",
    "email": "agraboso@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "lodash.isplainobject": "^4.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-istanbul": "^0.12.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-tape-runner": "^2.0.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.1",
    "eslint-plugin-prettier": "^2.6.0",
    "isomorphic-fetch": "^2.2.1",
    "nock": "^9.3.3",
    "prettier": "^1.11.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "size-limit": "^0.18.2",
    "tap": "^12.0.1",
    "tap-spec": "^5.0.0 ",
    "tape": "^4.9.1"
  },
  "files": [
    "README.md",
    "LICENSE.md",
    "es",
    "lib",
    "umd"
  ]
}
